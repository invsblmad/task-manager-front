{
    "name": "task-tracker",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^13.5.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-icons": "^5.5.0",
        "react-router-dom": "^7.6.0",
        "react-scripts": "5.0.1",
        "web-vitals": "^2.1.4",
        "zustand": "^5.0.4"
    },
    "scripts": {
        "prettier": "prettier",
        "start": "vite",
        "test": "react-scripts test",
        "prepare": "husky install",
        "dev": "vite",
        "build": "vite build",
        "serve": "vite preview",
        "lint": "eslint \"**/*.{js,jsx}\"",
        "lint:fix": "eslint \"**/*.{js,jsx}\" --fix"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "npm run lint:fix"
        ]
    },
    "type": "module",
    "devDependencies": {
        "@vitejs/plugin-react": "^4.4.1",
        "sass-embedded": "^1.88.0",
        "vite": "^6.3.5"
    }
}
